{
  "name": "fluxao",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "node scripts/start-dev.mjs",
    "dev:fixed": "next dev",
    "dev:clean": "node scripts/clear-cache.mjs && node scripts/start-dev.mjs",
    "clear-cache": "node scripts/clear-cache.mjs",
    "auto-backup": "node scripts/auto-backup.mjs",
    "dev:backup": "concurrently \"npm run auto-backup\" \"npm run dev\"",
    "git-history": "git log --oneline --graph --decorate",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --max-warnings=0",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:unit": "jest --testPathPattern=__tests__/components",
    "test:api": "jest --testPathPattern=__tests__/api",
    "test:integration": "jest --testPathPattern=__tests__/lib",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "test:newsletter": "vitest __tests__/newsletter",
    "worker:newsletter": "tsx server/queue/newsletter.job.ts",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "import:articles": "tsx scripts/import-articles.ts",
    "import:watch": "tsx scripts/import-articles.ts --watch",
    "import:update": "tsx scripts/import-articles.ts --update",
    "postbuild": "next-sitemap",
    "prepare": "husky",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop all",
    "pm2:restart": "pm2 restart all",
    "pm2:reload": "pm2 reload all",
    "pm2:logs": "pm2 logs",
    "pm2:status": "pm2 status",
    "setup": "sh scripts/setup-complete.sh",
    "deploy": "pnpm run build && pnpm run pm2:reload",
    "perf:optimize": "tsx scripts/performance-optimization.ts",
    "perf:monitor": "tsx lib/performance-monitor.ts",
    "perf:test": "tsx scripts/performance-test.js",
    "build:analyze": "ANALYZE=true npm run build"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
